unit UController;

interface

type
  Controller = class
  private
  protected
  public
    constructor CrieObj;
    destructor Destrua_se;
    procedure Salvar( PObj: Tobject );
    procedure Excluir( PObj: TObject );
    function Pesquisar( PChave: String ): string;
    function Carregar( PObj: TObject ): TObject;

  end;

implementation

end.
